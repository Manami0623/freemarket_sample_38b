.single-container
  =render""
  .single-main
    .l-single-container
      %h2.l-single-head
        新規会員登録
      .l-single-innner
        .l-single-content
          %button.default.btn-mail
            %i.far.fa-envelope
            = link_to "メールアドレスで登録",
              new_user_registration_path, class: 'post'
          %button#facebook-login.btn-default.btn-sns
          .btn-sns-facebook
            %i.fab.fa-facebook
            = link_to 'Facebookでログイン',
              user_facebook_omniauth_authorize_path
          %button#google-login.btn-default.btn-sns.btn-sns-google
            %i.fab.fa-google-plus-g
            = link_to 'Googleでログイン',
              user_google_oauth2_omniauth_authorize_path
  =render""